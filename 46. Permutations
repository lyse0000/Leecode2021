class Solution:
    def permute(self, nums: List[int]) -> List[List[int]]:
        if len(nums) == 1:
            return [nums]
        
        ret = []
        
        for i in range(len(nums)):
            others = nums[0:i]+nums[i+1:]
            other_permutation = self.permute(others)
            for p in other_permutation:
                ret.append([nums[i]]+p)
        return ret
